				{
					"Type": "NodeCodeBlockCode",
					"Data": "// 子组件\n\u003ctemplate\u003e\n  \u003c!-- 9. Vue3支持碎片(Fragments)，组件可以拥有多个根节点 --\u003e\n  \u003cdiv v-focus\u003e{{ newMsg }}\u003c/div\u003e\n  \u003cbutton @click=\"handleClick\"\u003eclick\u003c/button\u003e\n  \u003c!-- 10. Teleport --\u003e\n  \u003cTeleport to=\"#teleport-target\"\u003e\n    \u003cdiv\u003eTeleport\u003c/div\u003e\n  \u003c/Teleport\u003e\n\u003c/template\u003e\n\n\u003cscript setup\u003e\nimport {\n  ref,\n  reactive,\n  computed,\n  watch,\n  watchEffect,\n  defineProps,\n  defineEmits,\n  useAttrs,\n  inject,\n  defineExpose,\n  onBeforeMount,\n  onMounted,\n  onBeforeUpdate,\n  onUpdated,\n  onActivated,\n  onDeactivated,\n  onBeforeUnmount,\n  onUnmounted,\n  toRef,\n} from \"vue\";\nimport Mitt from \"@/utils/mitt.js\";\n\n// 1. ref/reactive\nlet msg = ref(\"hello\");\nlet obj = reactive({\n  name: \"obj\",\n});\n\n// 2. 生命周期函数\nonBeforeMount(() =\u003e {\n  console.log(\"onBeforeMount\");\n});\nonMounted(() =\u003e {\n  console.log(\"onMounted\");\n});\nonBeforeUpdate(() =\u003e {\n  console.log(\"onBeforeUpdate\");\n});\nonUpdated(() =\u003e {\n  console.log(\"onUpdated\");\n});\nonActivated(() =\u003e {\n  console.log(\"onActivated \");\n});\nonDeactivated(() =\u003e {\n  console.log(\"onDeactivated\");\n});\nonBeforeUnmount(() =\u003e {\n  console.log(\"onBeforeUnmount\");\n});\nonUnmounted(() =\u003e {\n  console.log(\"onUnmounted\");\n});\n\n// 3. computed\nconst newMsg = computed(() =\u003e {\n  return msg.value + \"world\";\n});\n\n// 4. watch\nwatch(msg, (newVal, oldVal) =\u003e {\n  console.log(newVal);\n  console.log(oldVal);\n});\n\n// 5. watchEffect\nwatchEffect(() =\u003e {\n  console.log(msg.value);\n});\n\n// 6.1 props/emit\nconst props = defineProps([\"parentValue\"]);\nconst parentValue = toRef(props, \"parentValue\");\nconsole.log(parentValue.value);\nconst emits = defineEmits([\"sendEmitValue\", \"update:modelValue\"]);\nconst sendEmitValue = () =\u003e {\n  emits(\"sendEmitValue\", \"emitValue\");\n};\n\n// 6.2 attrs/listeners\n// Vue3中已经不再使用$listeners\n// Vue3中使用attrs调用父组件方法时，方法前需要加上on\nconst attrs = useAttrs();\n//获取父组件方法和事件\nconsole.log(attrs.parentAttr); //Proxy {\"msg1\": \"子组件msg1\",\"msg2\": \"子组件msg2\"}\n//调用父组件方法\nattrs.onParentFun();\n\n// 6.3 provide/inject\n// 注入上层组件提供的数据\ninject(\"parentProvideValue\").value;\n\n// 6.4 parent/children 已移除\n\n// 6.5 ref/expose\nconst childExposeValue = ref(\"childExposeValue\");\nconst childExposeFun = () =\u003e {\n  console.log(\"childExposeFun\");\n};\ndefineExpose({\n  childExposeValue,\n  childExposeFun,\n});\n\n// 6.6 EventBus/mitt\nconst sendMittValue = () =\u003e {\n  Mitt.emit(\"sendMittValue\", \"mittValue\");\n};\n\n// 7. v-model，移除 .sync\nconst updateModelValue = () =\u003e {\n  emits(\"update:modelValue\", \"updateModelValue\");\n};\n\n// 8. 自定义指令\nconst vFocus = {\n  mounted: (el) =\u003e console.log(el),\n};\n\nfunction handleClick() {\n  sendEmitValue();\n  sendMittValue();\n  updateModelValue();\n}\n\u003c/script\u003e\n"
