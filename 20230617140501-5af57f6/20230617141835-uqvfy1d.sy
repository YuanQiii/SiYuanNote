										{
											"ID": "20230619114423-lslqbp2",
											"Type": "NodeParagraph",
											"Properties": {
												"id": "20230619114423-lslqbp2",
												"updated": "20230619114709"
											},
											"Children": [
												{
													"Type": "NodeText",
													"Data": "由于iOS13将uiwebview列入非公开api，并说明未来会禁止使用uiwebview的应用上架。所以从HBuilderX 2.2.5起，默认使用的是wkwebview，如果要切换为uiwebview，需要在manifest里或创建webview时的参数里指定kernel"
												}
											]
										}
									]
								},
								{
									"ID": "20230619114724-4nq18a8",
									"Type": "NodeListItem",
									"ListData": {
										"Typ": 1,
										"Delimiter": 46,
										"Marker": "My4=",
										"Num": 3
									},
									"Properties": {
